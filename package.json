{
  "name": "paal-workout-server",
  "version": "0.1.3",
  "description": "Node.js server for workout app",
  "main": "index.js",
  "engines": {
    "node": "9.8.0"
  },
  "scripts": {
    "deploy": "sh ./script.sh deploy",
