  "devDependencies": {
    "mocha": "^5.2.0",
    "nock": "^9.6.1",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "proxyquire": "^2.0.1",
    "sinon": "^6.1.5",
    "superagent-defaults": "^0.1.14",
    "supertest": "^3.1.0",
    "why-is-node-running": "^2.0.3"
